{
  "name": "olt",
  "version": "0.0.1",
  "description": "A dead simple, one-line template",
  "repository": "ambar/olt",
  "license": "MIT",
  "module": "index.mjs",
  "unpkg": "index.mjs",
  "main": "index.cjs.js",
  "files": [
    "index.cjs.js",
    "index.mjs"
  ],
  "keywords": [
    "olt",
    "template",
    "ejs",
    "mustache",
    "lodash.template"
  ],
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "test": "node --test",
    "test:coverage": "node --test --experimental-test-coverage --test-reporter=lcov --test-reporter-destination=./coverage/lcov.info",
    "test:watch": "node --test --watch --notify",
    "build": "rollup -f cjs $npm_package_module -o $npm_package_main"
  },
  "devDependencies": {
    "rollup": "^2.75.5"
  },
  "packageManager": "pnpm@8.13.0+sha256.fbcf256db6d06bc189e31df34b3ed61220f3ba9f78a2ca8fe7be0fce4670dbd3"
}
